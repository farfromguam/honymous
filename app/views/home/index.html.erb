<% if user_signed_in? %>
  <%= link_to('Log Out', destroy_user_session_path, :method => :delete) %>
<% else %>
  <%= link_to('Log In', new_user_session_path) %>
<% end %> <br>

<h1>Honymous</h1>
<p>
Current Profiles, <%= @profile_count %>
</p>

<p>
<% @all_profiles.each do |profile| %>
